  "contributors": [],
  "scripts": {
    "eslint": "eslint .",
    "rollup": "npm run eslint && rollup -c",
    "test-rollup": "rollup -c rollup.config.test.js",
    "browser-test": "npm run rollup && opn http://localhost:8091/test/ && npm run start",
    "test": "npm run rollup && npm run test-rollup && node test-helpers/loadTests-commonjs.js",
    "kill-static": "pkill -f \".bin/static -p\"",
    "test-no-open": "npm run kill-static ; npm run eslint && npm run rollup && npm run start",
    "start": "static -p 8091",
    "watch": "chokidar '**/*.js' -c 'npm run test-no-open' -i 'dist' -i 'node_modules' & npm run test-no-open"
  },
  "description": "Provide round-trippable JSON/JavaScript serialization",
  "main": "dist/jml-node.js",
  "browser": "src/jml.js",
  "module": "src/jml-node.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/brettz9/jamilih"
  },
  "homepage": "https://github.com/brettz9/jamilih",
  "keywords": [
    "json",
    "javascript",
    "template",
    "templating",
    "html"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=0.10"
  },
  "dependencies": {
    "jsdom": "12.0.0"
  },
  "devDependencies": {
    "babel-core": "6.26.3",
    "babel-plugin-add-module-exports": "^0.3.3",
    "babel-plugin-external-helpers": "6.22.0",
    "babel-preset-env": "1.7.0",
    "chokidar-cli": "^1.2.1",
    "eslint": "5.5.0",
    "eslint-config-standard": "12.0.0",
    "eslint-plugin-compat": "2.5.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-node": "7.0.1",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-standard": "4.0.0",
    "node-static": "0.7.10",
    "nodeunit": "0.11.3",
    "opn-cli": "^3.1.0",
    "remark-lint": "6.0.2",
    "rollup": "0.65.2",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "9.1.6",
    "rollup-plugin-json": "3.0.0",
    "rollup-plugin-node-resolve": "3.4.0",
    "rollup-plugin-re": "^1.0.7"
  },
  "readmeFilename": "README.md",
