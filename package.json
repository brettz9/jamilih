{
  "name": "jamilih",
  "version": "0.52.1",
  "author": {
    "name": "Brett Zamir",
    "email": "brettz9@yahoo.com",
    "url": "http://brett-zamir.me"
  },
  "contributors": [],
  "scripts": {
    "prepublishOnly": "npm run rollup",
    "license-badge": "license-badger --corrections --uncategorizedLicenseTemplate \"\\${license} (\\${name} (\\${version}))\" --filteredTypes=nonempty --textTemplate \"License types\n(project, deps, and bundled devDeps)\" --packageJson --production badges/licenses-badge.svg",
    "license-badge-dev": "license-badger --corrections --filteredTypes=nonempty --textTemplate \"License types\n(all devDeps)\" --allDevelopment badges/licenses-badge-dev.svg",
    "license-badges": "npm run license-badge && npm run license-badge-dev",
    "eslint": "eslint --ext=js,md,html .",
    "rollup": "rollup -c",
    "browser-test": "npm run rollup && open-cli http://localhost:8091/test/ && npm run start",
    "mocha": "mocha --require esm test-helpers/loadTests-node.js test/test.*.js --reporter-options configFile=mocha-multi-reporters.json",
    "test": "rm -Rf .nyc_output && rm -Rf coverage && rm -Rf node_modules/.cache && nyc npm run mocha && npm run coverage-badge",
    "kill-static": "pkill -f \".bin/static -p\"",
    "build-docs": "rm -rf docs/jsdoc/*;jsdoc --pedantic -c docs/jsdoc-config.js .",
    "open-docs": "open-cli http://localhost:8091/docs/jsdoc/ && npm start",
    "coverage-badge": "coveradge badges/coverage-badge.svg",
    "coverage": "open-cli http://localhost:8091/coverage/ && npm start",
    "test-no-open": "npm run kill-static ; npm run eslint && npm run rollup && npm run start",
    "start": "static -p 8091",
    "watch": "chokidar '**/*.js' -c 'npm run test-no-open' -i 'dist' -i 'node_modules' & npm run test-no-open"
  },
  "nyc": {
    "exclude": [
      "test",
      "test-helpers",
      ".mocharc.js"
    ],
    "reporter": [
      "text",
      "html",
      "json-summary"
    ]
  },
  "browserslist": [
    "cover 100%"
  ],
  "description": "Provide round-trippable JSON/JavaScript serialization",
  "main": "dist/jml-jsdom.js",
  "browser": "dist/jml.js",
  "module": "dist/jml-es.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/brettz9/jamilih"
  },
  "homepage": "https://github.com/brettz9/jamilih",
  "keywords": [
    "json",
    "javascript",
    "template",
    "templating",
    "html"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brettz9/jamilih/issues"
  },
  "engines": {
    "node": ">=10"
  },
  "peerDependencies": {
    "core-js-bundle": "^3.6.4"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@mysticatea/eslint-plugin": "^13.0.0",
    "@rollup/plugin-json": "^4.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "babel-plugin-add-module-exports": "^1.0.2",
    "chai": "^4.2.0",
    "chokidar-cli": "^2.1.0",
    "core-js-bundle": "^3.6.4",
    "coveradge": "^0.5.0",
    "eslint": "^6.8.0",
    "eslint-config-ash-nazg": "^18.0.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-array-func": "^3.1.4",
    "eslint-plugin-chai-expect": "^2.1.0",
    "eslint-plugin-chai-friendly": "^0.5.0",
    "eslint-plugin-compat": "^3.5.1",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsdoc": "^22.1.0",
    "eslint-plugin-markdown": "^1.0.2",
    "eslint-plugin-no-unsanitized": "^3.1.0",
    "eslint-plugin-no-use-extend-native": "^0.4.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-sonarjs": "^0.5.0",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-unicorn": "^18.0.1",
    "esm": "^3.2.25",
    "jsdoc": "^3.6.3",
    "jsdom": "16.2.1",
    "license-badger": "^0.12.6",
    "mocha": "^7.1.1",
    "mocha-badge-generator": "^0.5.2",
    "mocha-multi-reporters": "git+https://github.com/brettz9/mocha-multi-reporters.git#missing-reporter",
    "node-static": "0.7.11",
    "nyc": "^15.0.0",
    "open-cli": "^6.0.1",
    "remark-lint": "6.0.6",
    "request": "^2.88.2",
    "rollup": "2.2.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-re": "^1.0.7",
    "typescript": "^3.8.3"
  }
}
