{
  "name": "jamilih",
  "version": "0.47.0",
  "author": {
    "name": "Brett Zamir",
    "email": "brettz9@yahoo.com",
    "url": "http://brett-zamir.me"
  },
  "contributors": [],
  "scripts": {
    "eslint": "eslint --ext=js,md,html .",
    "rollup": "rollup -c",
    "browser-test": "npm run rollup && open-cli http://localhost:8091/test/ && npm run start",
    "test": "rm -Rf node_modules/.cache && nyc mocha --require esm test-helpers/loadTests-node.js test/test.*.js",
    "kill-static": "pkill -f \".bin/static -p\"",
    "build-docs": "rm -rf docs/jsdoc/*;jsdoc --pedantic -c docs/jsdoc-config.js .",
    "open-docs": "open-cli http://localhost:8091/docs/jsdoc/ && npm start",
    "coverage": "open-cli http://localhost:8091/coverage/ && npm start",
    "test-no-open": "npm run kill-static ; npm run eslint && npm run rollup && npm run start",
    "start": "static -p 8091",
    "watch": "chokidar '**/*.js' -c 'npm run test-no-open' -i 'dist' -i 'node_modules' & npm run test-no-open"
  },
  "nyc": {
    "exclude": [
      "test",
      "test-helpers"
    ],
    "reporter": [
      "text",
      "html"
    ]
  },
  "browserslist": [
    "cover 100%"
  ],
  "description": "Provide round-trippable JSON/JavaScript serialization",
  "main": "dist/jml-polyglot.js",
  "browser": "dist/jml.js",
  "module": "dist/jml-es.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/brettz9/jamilih"
  },
  "homepage": "https://github.com/brettz9/jamilih",
  "keywords": [
    "json",
    "javascript",
    "template",
    "templating",
    "html"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brettz9/jamilih/issues"
  },
  "engines": {
    "node": ">=0.10"
  },
  "peerDependencies": {
    "core-js-bundle": "^3.6.4"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@mysticatea/eslint-plugin": "^13.0.0",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "babel-plugin-add-module-exports": "^1.0.2",
    "chai": "^4.2.0",
    "chokidar-cli": "^2.1.0",
    "core-js-bundle": "^3.6.4",
    "eslint": "^6.8.0",
    "eslint-config-ash-nazg": "^16.5.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-array-func": "^3.1.3",
    "eslint-plugin-chai-expect": "^2.1.0",
    "eslint-plugin-chai-friendly": "^0.5.0",
    "eslint-plugin-compat": "^3.3.0",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsdoc": "^20.3.1",
    "eslint-plugin-markdown": "^1.0.1",
    "eslint-plugin-no-unsanitized": "^3.0.2",
    "eslint-plugin-no-use-extend-native": "^0.4.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-sonarjs": "^0.5.0",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-unicorn": "^15.0.1",
    "esm": "^3.2.25",
    "jsdoc": "^3.6.3",
    "jsdom": "16.0.0",
    "mocha": "^7.0.0",
    "node-static": "0.7.11",
    "nyc": "^15.0.0",
    "open-cli": "^5.0.0",
    "remark-lint": "6.0.5",
    "request": "^2.88.0",
    "rollup": "1.29.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "10.1.0",
